{
  "name": "frida-java",
  "version": "1.0.6",
  "description": "Java runtime interop from Frida",
  "main": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "es2015",
              {
                "loose": true
              }
            ]
          ],
          "plugins": [
            "transform-runtime"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frida/frida-java.git"
  },
  "license": "LGPL-2.0 WITH WxWindows-exception-3.1",
  "bugs": {
    "url": "https://github.com/frida/frida-java/issues"
  },
  "homepage": "https://github.com/frida/frida-java#readme",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.20.0"
  },
  "devDependencies": {
    "bluebird": "^3.4.6",
    "bluebird-co": "^2.2.0",
    "frida": "^9.0.1",
    "frida-compile": "^5.0.0",
    "mocha": "^3.1.2",
    "should": "^11.1.1"
  },
  "scripts": {
    "test": "frida-compile test/agent -o test/_agent.js && mocha test/android.js"
  }
}
